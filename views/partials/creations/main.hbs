<section style="background-color: #f6f9fe;">
    <div class="container">

        <div class="section-title">
          <h3><span>Mes Cr√©ations</span></h3>
        </div>

        <div class="row" style="background-color: #f6f9fe;" data-aos="fade-right">
            {{!-- Permet d'obtenir tout mes items afin de les afficher --}}
            {{#each creationsItem }}

            <div class="my-5 col-xs-12 col-xm-12 col-md-6 col-lg-3 d-flex justify-content-center"
                data-aos="zoom-out-down" data-aos-delay="{{this.id}}00">
                
                    <div class="card" style="width: 18rem;">
                        <a href="/creations/{{ this.id }}">
                        <div
                            style="background-image: url('/assets/images/creations/{{ this.img }}'); background-size: cover; background-position:center;">
                            <p style="height: 250px;"></p>
                        </div>
                        </a>
                        <div class="card-body">
                            <p class="card-text">{{ this.desc }}</p>
                            <p class="card-text"><i>Fait le :{{ this.date }}</i></p>
                        </div>
                    </div>
                
            </div>
            {{/each }}
        </div>
    </div>
</section>

